<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:paddingVertical="@dimen/pad_vertical_login"
    android:paddingHorizontal="@dimen/pad_vertical_login">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/home_rut"
            android:textSize="@dimen/subtitle_size"
            android:textColor="@color/textPrimary1">
        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/home_nombre"
            android:textSize="@dimen/subtitle_size"
            android:textColor="@color/textPrimary1">
        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/home_apellido"
            android:textSize="@dimen/subtitle_size"
            android:textColor="@color/textPrimary1">
        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/homa_fecha"
            android:textSize="@dimen/subtitle_size"
            android:textColor="@color/textPrimary1">
        </TextView>
    </LinearLayout>
    <ImageView
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:id="@+id/home_imagen"
        android:layout_gravity="center_vertical"
        android:layout_weight="1">
    </ImageView>



</LinearLayout>